{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "build",
  "routes": [
  {
    "src": "/(favicon\\.ico|manifest\\.json|logo192\\.png|logo512\\.png|robots\\.txt)",
    "dest": "/$1"
  },
  {
    "src": "/(.*)",
    "dest": "/index.html"
  }
]

}
